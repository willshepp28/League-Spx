{
  "name": "backend",
  "version": "1.0.0",
  "description": "league sponsorship web service",
  "main": "app.js",
  "scripts": {
    "test": "mocha",
    "start": "nodemon app.js",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "deploy": "git push heroku main",
    "readme": "git add . && git commit -m 'updated readme' && git push",
    "createdb": " sequelize db:create",
    "dropdb": "sequelize db:drop",
    "migrate": "sequelize db:migrate",
    "seedAll": "npx sequelize db:seed:all",
    "dropcreatedb": "sequelize db:drop && sequelize db:create"
  },
  "author": "Will Sheppard",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "sequelize-cli": "^6.2.0"
  }
}
